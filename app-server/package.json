{
  "name": "app-server",
  "prisma": {
    "schema": "./src/infra/db/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register -r dotenv-flow/config src/index.ts",
    "start": "ts-node-transpile-only -r tsconfig-paths/register src/index.ts",
    "db:dev": "pnpify prisma migrate dev",
    "db:deploy": "pnpify prisma migrate deploy",
    "db:generate": "pnpify prisma generate",
    "typespec": "yarn swagger:gen && yarn openapi-typescript src/routes/dto/swagger.json --output src/routes/dto/swagger.d.ts",
    "swagger:gen": "ts-node-transpile-only -r tsconfig-paths/register src/routes/api/swagger-generate.ts",
    "lint": "biome check --write ./",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@biomejs/biome": "1.9.4",
    "@jest/globals": "^29.7.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/bcrypt": "^5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4",
    "@types/jsonwebtoken": "^9",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6",
    "@types/swagger-ui-express": "^4.1.6",
    "@yarnpkg/pnpify": "^4.1.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prisma": "6.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5"
  },
  "dependencies": {
    "@prisma/client": "6.1.0",
    "axios": "^1.7.9",
    "dotenv-flow": "^4.1.0",
    "envalid": "^8.0.0",
    "express": "^4.21.2",
    "express-openapi-validator": "^5.4.3",
    "http-status-codes": "^2.3.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^7.5.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "zod": "^3.24.1"
  }
}
